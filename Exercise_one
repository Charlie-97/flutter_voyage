8.	class Car{
		final String brandName;
		final String model;
		final String color;
		final int year;
		
		const Car({
			required this.brandName,
			required this.model,
			required this.color,
			required this.year,
	}
	
	class Main{
		final Car myCar = Car(brandName: 'Dodge', model: 'Challenger', color: 'black', year: 2022,);
		
		final Car yourCar = Car(brandName: 'Toyota', model: 'Venza', color: 'wine', year: 2024,);
		
		final Car lastCar = Car(brandName: 'Ford', model: 'Avalanche', color: 'black', year: 2019,);
	}
	
